{
  "name": "food",
  "version": "1.0.0",
  "description": "Vegy food shop",
  "main": "index.html",
  "scripts": {
    "webpack": "npx webpack",
    "prefix": "postcss --use autoprefixer -b 'last 10 versions' css/style.css -o css/style.css",
    "compress": "node-sass css/style.css css/style.css --output-style compressed",
    "build": "npm-run-all prefix compress webpack",
    "server": "http-server -o",
    "compile": "node-sass scss/style.scss css/style.css -w",
    "dev": "concurrently --kill-others \"npm run server\" \"npm run compile\" \"npm run webpack\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mux-mux/food.git"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/mux-mux/food#readme",
  "browserslist": [
    "> 1%"
  ],
  "devDependencies": {
    "@babel/cli": "^7.21.0",
    "@babel/core": "^7.21.3",
    "@babel/preset-env": "^7.20.2",
    "babel-loader": "^9.1.2",
    "core-js": "^3.29.1",
    "json-server": "^0.17.3",
    "webpack-cli": "^5.1.4",
    "autoprefixer": "^10.4.14",
    "concurrently": "^8.2.0",
    "http-server": "^14.1.1",
    "node-sass": "^8.0.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0"
  },
  "dependencies": {
    "es6-promise-polyfill": "^1.2.0",
    "nodelist-foreach-polyfill": "^1.2.0"
  }
}
